<?xml version="1.0"?>
<PropertyList>
  <path>alt.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>ALT.needle100</object-name>
    <property>/instrumentation/altimeter/indicated-altitude-ft</property>
    <min>0</min>
    <max>65617</max> <!-- 20,000 m -->
    <interpolation>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>65617</ind><dep>1440</dep></entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>ALT.needle1000</object-name>
    <property>/instrumentation/altimeter/indicated-altitude-ft</property>
    <min>0</min>
    <max>65617</max> <!-- 20,000 m -->
    <interpolation>
      <entry><ind>0</ind><dep>0</dep></entry>
      <entry><ind>65617</ind><dep>144</dep></entry>
    </interpolation>
    <center>
      <x-m>0</x-m>
      <y-m>0</y-m>
      <z-m>0</z-m>
    </center>
    <axis>
      <x>-1</x>
      <y>0</y>
      <z>0</z>
    </axis>
  </animation>
</PropertyList>
