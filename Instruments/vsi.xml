<?xml version="1.0"?>

<PropertyList>

 <path>vsi.ac</path>

  
  <animation>
  <type>material</type>
    <object-name>VSI.face</object-name>
    <object-name>VSI.needle</object-name>
    <global type="bool">true</global>
  <emission>
    <red>0.5</red>
    <green>0.2</green>
    <blue>0.0</blue>
    <factor-prop>/controls/lighting/instruments-norm</factor-prop>
  </emission>
</animation>

  <animation>
  <type>rotate</type>
  <object-name>VSI.needle</object-name>
  <property>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
  <interpolation>
   <entry><ind>  -10000.0</ind><dep> -180.0</dep></entry>
   <entry><ind>   -5000.0</ind><dep> -90.0</dep></entry>
   <entry><ind>      0.0</ind><dep>   0.0</dep></entry>
   <entry><ind>    5000.0</ind><dep>  90.0</dep></entry>
   <entry><ind>   10000.0</ind><dep>  180.0</dep></entry>
  </interpolation>     
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
 </animation>
 
</PropertyList>