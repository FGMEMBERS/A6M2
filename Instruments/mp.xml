<?xml version="1.0"?>
<PropertyList>

 <path>mp.ac</path>

 <animation>
  <type>rotate</type>
  <object-name>MAN.needle</object-name>
  <property>engines/engine/mp-osi</property>
  <interpolation>
   <entry><ind>-45</ind><dep>-203</dep></entry>
   <entry><ind>0</ind><dep>0</dep></entry>
   <entry><ind>25</ind><dep>113</dep></entry>
  </interpolation>
  <center>
   <x-m>0</x-m>
   <y-m>0</y-m>
   <z-m>0</z-m>
  </center>
  <axis>
   <x>-1</x>
   <y>0</y>
   <z>0</z>
  </axis>
 </animation>

</PropertyList>
